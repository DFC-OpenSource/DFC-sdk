/dts-v1/;

#include "fsl-ls1088a.dtsi"

/ {
	model = "Freescale Layerscape 1088a RDB Board";
	compatible = "fsl,ls1088a-rdb", "fsl,ls1088a";
	aliases {
		duart0 = &qspi;
		spi1 = &dspi;
	};
};

&qspi {
	bus-num = <0>;
	status = "okay";

	qflash0: s25fl128s@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-flash";
		spi-max-frequency = <20000000>;
		reg = <0>;
	};

	qflash1: s25fl128s@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-flash";
		spi-max-frequency = <20000000>;
		reg = <1>;
	};
};
