 gas/config/tc-mips.c  |    7 ++++++-
 gas/configure         |    3 +++
 gas/configure.tgt     |    2 +-
 gas/doc/c-mips.texi   |    3 ++-
 include/elf/mips.h    |    1 +
 include/opcode/mips.h |    6 +++++-
 ld/configure.tgt      |    2 ++
 opcodes/mips-dis.c    |    6 ++++++
 opcodes/mips-opc.c    |   31 ++++++++++++++++++++-----------
 17 files changed, 73 insertions(+), 17 deletions(-)

