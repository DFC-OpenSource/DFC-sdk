 /* Return true if the given CPU is included in INSN_* mask MASK.  */
 
@@ -1239,6 +1242,9 @@
     case CPU_XLR:
       return (mask & INSN_XLR) != 0;
 
+    case CPU_XLP:
+      return (mask & INSN_XLP) != 0;
+
     default:
       return FALSE;
     }
